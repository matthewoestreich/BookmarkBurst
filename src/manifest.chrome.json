{
  "manifest_version": 3,
  "name": "BookmarkBurst",
  "version": "1.0",
  "description": "Browser extension for various bookmark related tasks.",
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icons/icon.png"
  }
}
